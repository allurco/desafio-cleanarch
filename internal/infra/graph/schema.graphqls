type Order {
    id: String!
    Price: Float!
    Tax: Float!
    FinalPrice: Float!
}

input OrderInput {
    id: String!
    Price: Float!
    Tax: Float!
}

input OrderListInput {
    page: Int
    limit: Int
    sort: String
}

type Query {
    order(input: OrderListInput): [Order!]!
}

type Mutation {
    createOrder(input: OrderInput): Order
}